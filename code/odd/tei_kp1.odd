                <!-- required textstructure elements (TEI, text, and body) -->
                <moduleRef include="TEI text body byline div group back closer dateline front                     opener argument docAuthor docDate docEdition docImprint docTitle epigraph                     floatingText imprimatur postscript salute signed titlePage titlePart trailer" key="textstructure"/>
                <!-- required module tei instantiates lots of classes used for further expansion of this odd -->
                <moduleRef key="tei"/>
                <moduleRef include="ab when link alt altGrp anchor annotation join joinGrp linkGrp                     listAnnotation seg standOff timeline" key="linking"/>
                <moduleRef include="org orgName persName person place addName affiliation age birth                     bloc climate country death district education event faith floruit forename                     gender genName geo geogFeat geogName langKnowledge langKnown listEvent listNym                     listObject listOrg listPerson listPlace listRelation location nameLink                     nationality nym object objectIdentifier objectName occupation offset persona                     personGrp persPronouns placeName population region relation residence roleName                     settlement sex socecStatus state surname terrain trait" key="namesdates"/>
