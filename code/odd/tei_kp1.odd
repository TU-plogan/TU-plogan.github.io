                <moduleRef include="xr" key="dictionaries"/>
                <moduleRef include="gi tag code alternate altIdent anyElement att attDef attList                     attRef classes classRef classSpec constraint constraintSpec content dataFacet                     dataRef dataSpec datatype defaultVal eg egXML elementRef elementSpec empty equiv                     exemplum ident listRef macroRef macroSpec memberOf model modelGrp modelSequence                     moduleRef moduleSpec outputRendition param paramList paramSpec remarks                     schemaSpec sequence specDesc specGrp specGrpRef specList textNode val valDesc                     valItem valList" key="tagdocs"/>
